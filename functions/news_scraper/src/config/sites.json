      {
        "parentSelector": ".story-card",
        "maxLinksToFollow": 3,
        "maxElementsToScrape": 3,
        "childSelectors": {
          "headline": ".c-card__hed-text",
          "link": "a"
        },
        "linkHandling": {
          "follow": true,
          "completeLink": true,
          "linkPrefix": "https://www.theglobeandmail.com",
          "parentSelector": "article.l-article",
          "childSelector": {
            "content": "p"
          }
        
        }
      }
    ]
  },

  "theGlobeAndMailWorld": {
    "url": "https://www.theglobeandmail.com/world/",
    "selectors": [
      {
        "parentSelector": ".story-card",
        "maxLinksToFollow": 3,
        "maxElementsToScrape": 3,
        "childSelectors": {
          "headline": ".c-card__hed-text",
          "link": "a"
        },
        "linkHandling": {
          "follow": true,
          "completeLink": true,
          "linkPrefix": "https://www.theglobeandmail.com",
          "parentSelector": "article.l-article",
